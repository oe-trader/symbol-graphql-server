{
  "name": "symbol-graphql-server",
  "version": "1.0.0",
  "description": "GraphQL server for Symbol blockchain",
  "main": "index.js",
  "scripts": {
    "generate": "node -r dotenv/config ./node_modules/.bin/mesh generate",
    "start": "node -r dotenv/config ./node_modules/.bin/mesh dev",
    "build": "node -r dotenv/config ./node_modules/.bin/mesh build",
    "swagger": "node -r dotenv/config ./scripts/swagger.js"
  },
  "keywords": [
    "symbol",
    "blockchain",
    "graphql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@graphql-mesh/cli": "^0.95.2",
    "@graphql-mesh/openapi": "^0.106.3",
    "@graphql-mesh/transform-rename": "^0.102.2",
    "graphql": "^16.9.0",
    "swagger-ui-express": "^5.0.1",
    "symbol-sdk": "^3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@symbol-blockchain-community/symbol-rest-client": "^1.0.2",
    "@types/node": "^22.5.0",
    "dotenv": "^16.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
