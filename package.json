{
  "name": "symbol-graphql-server",
  "version": "1.0.0",
  "description": "GraphQL server for Symbol blockchain",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "generate": "node -r dotenv/config ./node_modules/.bin/mesh generate",
    "dev": "node -r dotenv/config ./node_modules/.bin/mesh dev",
    "build": "node -r dotenv/config ./node_modules/.bin/mesh build",
    "swagger": "node -r dotenv/config ./scripts/swagger.js"
  },
  "keywords": [
    "symbol",
    "blockchain",
    "graphql"
  ],
  "license": "MIT",
  "devDependencies": {
    "dotenv": "^16.4.7",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "yamljs": "^0.3.0"
  },
  "dependencies": {
    "@graphql-mesh/cli": "^0.96.12",
    "@graphql-mesh/openapi": "^0.108.6",
    "@graphql-mesh/transform-rename": "^0.102.13",
    "graphql": "^16.9.0",
    "symbol-sdk": "~3.2.3"
  }
}
