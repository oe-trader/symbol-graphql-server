{
  "name": "symbol-graphql-server",
  "version": "1.0.0",
  "description": "GraphQL server for Symbol blockchain",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [
    "symbol",
    "blockchain",
    "graphql"
  ],
  "scripts": {
    "generate": "node -r dotenv/config ./node_modules/.bin/mesh generate",
    "start": "node -r dotenv/config ./node_modules/.bin/mesh dev",
    "build": "node -r dotenv/config ./node_modules/.bin/mesh build",
    "validate": "node -r dotenv/config ./node_modules/.bin/mesh validate",
    "swagger": "tsx scripts/swagger.mts"
  },
  "dependencies": {
    "@graphql-mesh/cli": "^0.98.7",
    "@graphql-mesh/openapi": "^0.108.6",
    "@graphql-mesh/transform-rename": "^0.103.6",
    "graphql": "^16.9.0",
    "symbol-sdk": "^3.2.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "dotenv": "^16.4.7",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "yamljs": "^0.3.0",
    "zod": "^3.23.8"
  }
}
